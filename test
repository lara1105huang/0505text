
<script src='//ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js'></script>

<script type="text/javascript"> 

    $(function () {

    var totalInput = $('#zmoney');

    $('.moneys').delegate('.cassmoney', 'keyup', function () {

        var total = 0;

        // 相加所有input的值

        $('.cassmoney').each(function () {

            var v = parseInt(this.value);

            if (!isNaN(v)) { // 如果v是有效数字

                total += v;

                this.value = v;

            } else {

                this.value = '';

            }

        });

        totalInput.val(total);

    });

});

</script> 

<body> 

<div class="moneys">

<input id="Lmoney1" name="Lmoney"  class="cassmoney"/>

<input id="Lmoney2" name="Lmoney"  class="cassmoney"/>

<input id="Lmoney3" name="Lmoney"  class="cassmoney"/>

<input id="Lmoney4" name="Lmoney"  class="cassmoney"/>

<input id="Lmoney5" name="Lmoney"  class="cassmoney"/>

</div>

<input  id='zmoney' name='zmoney'  class='zcassmoney'/>

</body>

</html>